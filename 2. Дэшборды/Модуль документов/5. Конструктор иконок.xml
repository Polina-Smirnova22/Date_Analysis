            <navsql>
                <query>
SELECT
    id_image,
    name_image,
    1.000000 / (COUNT(*) OVER() + 1) AS Share_to_widht_container,
    ROW_NUMBER() OVER() AS sort,
    CASE
        WHEN name_image = [**tab1]
        THEN '#7C95AB70'
        ELSE null
        END AS color
FROM (
    VALUES
    ('1021472','Главная'),
    ('1021473', 'Архив')
) AS t (id_image, name_image)</query>
